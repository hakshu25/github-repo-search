{
  "name": "github-repo-search",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "test:unit": "vitest run --coverage",
    "lint": "eslint src/**/*.{js,vue} tests/unit/**/*",
    "format": "prettier --write src/**/*.{js,vue} tests/**/*",
    "format:check": "prettier --check src/**/*.{js,vue} tests/**/*",
    "prepare": "husky"
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.5.13",
    "lodash": "4.17.21",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/eslint-parser": "^7.17.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/vue": "^8.0.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitest/coverage-v8": "^3.0.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-standard": "9.0.1",
    "axios": "1.8.4",
    "eslint": "8.57.1",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-vue": "9.33.0",
    "husky": "^9.0.0",
    "jsdom": "^26.0.0",
    "lint-staged": ">=10",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "sass": "1.86.0",
    "tailwindcss": "^4.0.0",
    "vite": "6.2.4",
    "vitest": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix"
  }
}
