{
  "name": "github-repo-search",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "test:unit": "vitest run --coverage",
    "lint": "eslint src/**/*.{js,vue} tests/unit/**/*",
    "format": "prettier --write src/**/*.{js,vue} tests/**/*",
    "format:check": "prettier --check src/**/*.{js,vue} tests/**/*",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.3.13",
    "lodash": "4.17.21",
    "vue": "3.3.13"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/eslint-parser": "^7.17.0",
    "@testing-library/vue": "^8.0.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-v8": "^0.34.1",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-standard": "8.0.1",
    "autoprefixer": "10.4.18",
    "axios": "1.6.7",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.22.0",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": ">=10",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "sass": "1.71.1",
    "tailwindcss": "3.4.1",
    "vite": "5.1.5",
    "vitest": "^0.34.0"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix"
  }
}
